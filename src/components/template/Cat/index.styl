@media screen and (min-width: 1440px) {
  /* Elemento | http://localhost:8097/top10 */
  .TopBreeds, .search h2 {
    font: 700 48px / 58px Montserrat;
  }

  .catList {
    margin-bottom: 106px;

    & a {
      font: var(--f-link);
      text-decoration: none;
      color: #291507;
    }
  }

  .cat {
    display: grid;
    grid-auto-flow: column;
    gap: 50px;
    margin: 56px 0;

    & figure {
      width: 170px;
      height: 170px;
    }

    & img {
      width: 100%;
      height: 100%;
      object-fit: cover;
      border-radius: 24px;
    }

    & h3 {
      font: 600 36px / 43px Montserrat;
    }

    & p {
      font: var(--parrafo-p);
    }
  }

  .catData {
    gap: 24px;
    display: grid;
    grid-auto-rows: min-content;
  }
}